{% extends 'base.html.twig' %}

{% block title %}
    Inscription - {{ parent() }}
{% endblock %}

{% block body %}
    <h1>Inscription</h1>

    {{ form_start(form) }}

    {{ form_row(form.email) }}
    {{ form_row(form.plainPassword) }}
    {{ form_row(form.pictureFile) }}
    {{ form_row(form.acceptTerms) }}

    <button class="btn btn-primary">
        S'inscrire
    </button>
    {{ form_end(form) }}
{% endblock %}
